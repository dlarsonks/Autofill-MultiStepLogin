<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:tools="http://schemas.android.com/tools"
    package="com.test.autofill"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- so we can query for browsers that support custom tabs -->
    <uses-permission android:name="android.permission.QUERY_ALL_PACKAGES"
        tools:ignore="QueryAllPackagesPermission" />

    <application
        android:allowBackup="false"
        android:fullBackupContent="@xml/full_backup_content"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.M3"
        android:testOnly="false">


        <activity
            android:name=".multisteplogin.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".multisteplogin.MultiLoginActivity"/>
        <activity android:name=".multisteplogin.DoneActivity"/>
        <activity android:name=".multisteplogin.username_and_password.UsernamePasswordTogetherActivity" />
        <activity android:name=".multisteplogin.MainProfileActivity" />
        <activity android:name=".multisteplogin.otp.OtpActivity" />
        <activity android:name=".multisteplogin.otp.OtpMultiActivity" />
        <activity android:name=".multisteplogin.webview.WebViewActivity" />
        <activity android:name=".multisteplogin.otp.OtpUsernamePasswordActivity" />
        <activity android:name=".multisteplogin.passwordnotvisible.PasswordNotVisibleActivity" />

        <activity
            android:name=".multisteplogin.customtab.CustomTabActivity"
            android:exported="false"
            android:theme="@style/AppTheme.M3" />

    </application>

</manifest>